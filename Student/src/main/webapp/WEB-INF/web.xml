<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>NewStudent11</display-name>
  <welcome-file-list>

    <welcome-file>Login.jsp</welcome-file>
   
  </welcome-file-list>
    
  <error-page>
  <error-code>404</error-code>
  <location>/Error1.jsp</location>
  </error-page>

<servlet>
		<servlet-name>LoginServletMap</servlet-name>
		<servlet-class>com.user.web.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServletMap</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ListControllerMap</servlet-name>
		<servlet-class>com.user.web.ListController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListControllerMap</servlet-name>
		<url-pattern>/list</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>InsertServletMap</servlet-name>
		<servlet-class>com.user.web.InsertServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InsertServletMap</servlet-name>
		<url-pattern>/insert</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>EditServletMap</servlet-name>
		<servlet-class>com.user.web.EditServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EditServletMap</servlet-name>
		<url-pattern>/edit</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>UpdateServletMap</servlet-name>
		<servlet-class>com.user.web.UpdateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateServletMap</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	
		<servlet>
		<servlet-name>NewServletMap</servlet-name>
		<servlet-class>com.user.web.NewServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NewServletMap</servlet-name>
		<url-pattern>/new</url-pattern>
	</servlet-mapping>
	
	
		
		<servlet>
		<servlet-name>DeleteServletMap</servlet-name>
		<servlet-class>com.user.web.DeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteServletMap</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
</web-app>